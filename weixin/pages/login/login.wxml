<!--pages/memberZ/memberZ.wxml-->
<!-- 用户登陆页面 -->
<view class="member">
  <view class="header-title"><text class="header-title-text">用户登陆</text></view>
  <view class="member-input">
    <view class="member-input-list">
      <text class="label">用户名</text><input type="text" class="input" bindinput='getUserName'/>
    </view>
    <view class="member-input-list">
      <text class="label">密码</text><input type="text" class="input" bindinput='getUserPass'/>
    </view>
    
  </view>
  <view class="member-botton">
    <button class="member-bt" bindtap="login">注册</button>
  </view>
</view>
